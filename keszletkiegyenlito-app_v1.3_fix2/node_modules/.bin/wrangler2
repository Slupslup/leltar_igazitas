#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/balazs/Documents/projekte/leltar_igazitas_v1.3/node_modules/.pnpm/wrangler@3.114.8_@cloudflare+workers-types@4.20250502.0/node_modules/wrangler/bin/node_modules:/Users/balazs/Documents/projekte/leltar_igazitas_v1.3/node_modules/.pnpm/wrangler@3.114.8_@cloudflare+workers-types@4.20250502.0/node_modules/wrangler/node_modules:/Users/balazs/Documents/projekte/leltar_igazitas_v1.3/node_modules/.pnpm/wrangler@3.114.8_@cloudflare+workers-types@4.20250502.0/node_modules:/Users/balazs/Documents/projekte/leltar_igazitas_v1.3/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/balazs/Documents/projekte/leltar_igazitas_v1.3/node_modules/.pnpm/wrangler@3.114.8_@cloudflare+workers-types@4.20250502.0/node_modules/wrangler/bin/node_modules:/Users/balazs/Documents/projekte/leltar_igazitas_v1.3/node_modules/.pnpm/wrangler@3.114.8_@cloudflare+workers-types@4.20250502.0/node_modules/wrangler/node_modules:/Users/balazs/Documents/projekte/leltar_igazitas_v1.3/node_modules/.pnpm/wrangler@3.114.8_@cloudflare+workers-types@4.20250502.0/node_modules:/Users/balazs/Documents/projekte/leltar_igazitas_v1.3/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/wrangler@3.114.8_@cloudflare+workers-types@4.20250502.0/node_modules/wrangler/bin/wrangler.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/wrangler@3.114.8_@cloudflare+workers-types@4.20250502.0/node_modules/wrangler/bin/wrangler.js" "$@"
fi
